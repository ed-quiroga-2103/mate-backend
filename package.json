{
    "name": "express-server-ts",
    "version": "1.0.3",
    "description": "express server",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build",
        "clean": "tsc --build --clean",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "npx tsc",
        "dev": "npx prisma generate && nodemon src/index.ts",
        "start": "npx ts-node dist/index.js",
        "prisma:pull": "npx prisma db pull && npmx prisma generate",
        "prisma:gen": "npx prisma generate",
        "paths:add": "npx ts-node scripts/addPath.ts"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.5",
        "prettier": "^2.5.1",
        "prisma": "^3.13.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@prisma/client": "^3.13.0",
        "@types/mongoose": "^5.11.97",
        "aws-sdk": "^2.1069.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "formidable": "^2.0.1",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.1.3",
        "mongoose": "^6.0.11",
        "multer": "^1.4.3",
        "multer-s3": "^2.10.0",
        "nodemon": "^2.0.15"
    }
}
